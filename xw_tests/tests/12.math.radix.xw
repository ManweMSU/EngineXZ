	arccos(frac3(-1.1, -2.2, -3.3));
	arccos(frac4(-1.1, -2.2, -3.3, -4.4));

	arctg(-1.1);
	arctg(frac2(-1.1, -2.2));
	arctg(frac3(-1.1, -2.2, -3.3));
	arctg(frac4(-1.1, -2.2, -3.3, -4.4));

	arcctg(-1.1);
	arcctg(frac2(-1.1, -2.2));
	arcctg(frac3(-1.1, -2.2, -3.3));
	arcctg(frac4(-1.1, -2.2, -3.3, -4.4));
}