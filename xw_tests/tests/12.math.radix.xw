ute math;

functio vertex V(~frac4 p : positus)
{
	p = frac4(0.0);
	
	radix(-1.1);
	radix(frac2(-1.1, -2.2));
	radix(frac3(-1.1, -2.2, -3.3));
	radix(frac4(-1.1, -2.2, -3.3, -4.4));

	radix_inversa(-1.1);
	radix_inversa(frac2(-1.1, -2.2));
	radix_inversa(frac3(-1.1, -2.2, -3.3));
	radix_inversa(frac4(-1.1, -2.2, -3.3, -4.4));

	pot(-1.1, 1.1);
	pot(frac2(-1.1, -2.2), frac2(1.1, 2.2));
	pot(frac3(-1.1, -2.2, -3.3), frac3(1.1, 2.2, 3.3));
	pot(frac4(-1.1, -2.2, -3.3, -4.4), frac4(1.1, 2.2, 3.3, 4.4));

	onera(-1.1, 1);
	onera(frac2(-1.1, -2.2), int2(1, 2));
	onera(frac3(-1.1, -2.2, -3.3), int3(1, 2, 3));
	onera(frac4(-1.1, -2.2, -3.3, -4.4), int4(1, 2, 3, 4));

	frac A;
	frac2 B;
	frac3 C;
	frac4 D;

	decompone(-1.1, A);
	decompone(frac2(-1.1, -2.2), B);
	decompone(frac3(-1.1, -2.2, -3.3), C);
	decompone(frac4(-1.1, -2.2, -3.3, -4.4), D);

	exp(-1.1);
	exp(frac2(-1.1, -2.2));
	exp(frac3(-1.1, -2.2, -3.3));
	exp(frac4(-1.1, -2.2, -3.3, -4.4));

	exp2(-1.1);
	exp2(frac2(-1.1, -2.2));
	exp2(frac3(-1.1, -2.2, -3.3));
	exp2(frac4(-1.1, -2.2, -3.3, -4.4));

	ln(-1.1);
	ln(frac2(-1.1, -2.2));
	ln(frac3(-1.1, -2.2, -3.3));
	ln(frac4(-1.1, -2.2, -3.3, -4.4));

	lb(-1.1);
	lb(frac2(-1.1, -2.2));
	lb(frac3(-1.1, -2.2, -3.3));
	lb(frac4(-1.1, -2.2, -3.3, -4.4));

	lg(-1.1);
	lg(frac2(-1.1, -2.2));
	lg(frac3(-1.1, -2.2, -3.3));
	lg(frac4(-1.1, -2.2, -3.3, -4.4));
}
