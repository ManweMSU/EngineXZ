ute math;

structura T {
	frac3 a;
	frac4 b;
}

functio vertex V(~frac4 p : positus, series[T] t : series)
{
	p = frac4(0.0);
	var e = t[5];
	var A = e.a;
	var B = e.b;
}
functio punctum P1(~frac4 p : color, textura_1d[int] it : textura, textura_1d[nint] ut : textura, textura_1d[frac] ft : textura, exceptor ex : exceptor)
{
	p = frac4(0.0);
	nint lat, mip;
	it.dimensiones(3, lat, mip);
	ut.dimensiones(3, lat, mip);
	ft.dimensiones(3, lat, mip);
	var L1 = it[0, 1];
	var E1 = it(ex, 0.5);
	var L2 = ut[0, 1];
	var E2 = ut(ex, 0.5);
	var L3 = ft[0, 1];
	var E3 = ft(ex, 0.5);
}
