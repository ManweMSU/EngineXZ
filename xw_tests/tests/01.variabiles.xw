structura T1 {
	frac A;
	int B;
}
structura NT1 {
	frac A;
	int B;
	structor () { A = 0.0; B = 0; }
}
structura NT2 {
	NT1 nt;
	int C;
}

functio vertex V(~frac4 p : positus)
{
	p = frac4(0.0);
	logicum A;
	int B;
	nint C;
	frac D;
	logicum2 E;
	int2 F;
	nint2 G;
	frac2 H;
}
functio punctum P(~frac4 p : color)
{
	p = frac4(0.0);
	logicum3 E;
	int3 F;
	nint3 G;
	frac3 H;
	logicum4 A;
	int4 B;
	nint4 C;
	frac4 D;
	frac2x2 M2;
	frac3x3 M3;
	frac4x4 M4;
	T1 t1;
	NT1 nt1;
	NT2 nt2;
}