functio vertex V(~frac4 p : positus)
{
	p = frac4(0.0);
	logicum A1 = sic, B1 = non;
	logicum2 A2 = logicum2(A1, B1), B2 = logicum2(B1, A1);
	logicum3 A3 = logicum3(A2, A1), B3 = logicum3(A1, B1, A1);
	logicum4 A4 = logicum4(A2, A2), B4 = logicum4(B1, B2, A1);

	-A1; -A2; -A3; -A4;
	~A1; ~A2; ~A3; ~A4;
	!A1; !A2; !A3; !A4;
	A1 | B1; A2 | B2; A3 | B3; A4 | B4;
	A1 & B1; A2 & B2; A3 & B3; A4 & B4;
	A1 # B1; A2 # B2; A3 # B3; A4 # B4;
	A1 |= B1; A2 |= B2; A3 |= B3; A4 |= B4;
	A1 &= B1; A2 &= B2; A3 &= B3; A4 &= B4;
	A1 #= B1; A2 #= B2; A3 #= B3; A4 #= B4;
	A1 == B1; A2 == B2; A3 == B3; A4 == B4;
	A1 != B1; A2 != B2; A3 != B3; A4 != B4;
	A1 < B1; A2 < B2; A3 < B3; A4 < B4;
	A1 > B1; A2 > B2; A3 > B3; A4 > B4;
	A1 <= B1; A2 <= B2; A3 <= B3; A4 <= B4;
	A1 >= B1; A2 >= B2; A3 >= B3; A4 >= B4;
	A2.omnis; A3.omnis; A4.omnis;
	A2.ullus; A3.ullus; A4.ullus;
	A2[0]; A3[1]; A4[2];
}
functio punctum P(~frac4 p : color)
{
	p = frac4(0.0);
}