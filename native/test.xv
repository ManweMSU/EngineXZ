auxilium attributum ("NomenModuli") = "MODULUS EXPERIMENTI";
auxilium attributum ("CreatorModuli") = "Engine Software";
auxilium attributum ("IuraExempli") = "© Engine Software. 2024";
auxilium attributum ("Versio") = "1.0.0.1";

auxilium icon (1) = "../artworks/xn_icon.eiwv";

importa consolatorium;
importa limae;

//[[systema] graphica]

var ~ consolatorium con = consolatorium.currens()^;

ute winapi;

functio nihil ini() introitus iacit {
	con.titulus = "Привет! \x1F337";
	proba {
		var fl = crea_flumenum_limae("CR.TXT", modus_limae.legere, modus_creatoris.aperi_praesens);
		var ddx = scriptio.decodifica(fl);
		con << ddx^.lege_cunctos() << "\n";
		con << linea.ex(ddx^.codex) << "\n";
	} cape (nintadl e, nintadl se) {
		con.color_primus = consolatorium.color.ruber;
		con << "ERROR " << e << ", SUBERROR " << se << "\n";
		con.color_primus = consolatorium.color.defalta;
		iace e, se;
	}
	// responde;
	
	var aux = para_auxilium(nullus, "", 1);
	var mem = aux^.lege_ordinem();

	con << linea.ex(aux) << "\n";
	con << linea.ex(mem) << "\n";

	ordo linea ol;
	ol << linea(mem^, -1, linea.codex.utf8);
	ol << (ol[0] + " ?");
	ol << ol[1].surroga(" ", "💟💟");
	var data = ol[0].codifica(linea.codex.utf8, sic);
	ol << linea.ex(ol[0].scinde(' '));

	var valor = linea("234=555555555555555").ad_frac("=");

	MessageBox(0, linea_utf16(linea.ex(ol) + " " + linea(valor)), linea_utf16(ol[1]), 0);

	InitCommonControls();
	MessageBox(0,
		linea_utf16("Сука \x1F49C\x1F337 привет!"),
		linea_utf16(linea(char('\x1F337'), 15)),
		//linea_utf16("Заголовок"),
		0x00000040);
}